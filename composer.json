{
    "name": "zuni/datatablebundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony DataTableBundle",
    "keywords": ["table","datatable"],
    "homepage": "https://github.com/zunitec/DataTableBundle",
    "authors": [
        {
            "name": "Zunitec Tecnologias",
